{
  "name": "hello-world",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "postinstall": "ng build --configuration production",
    "deploy:gh": "ng build --configuration production && npx angular-cli-ghpages --dir=dist/hello-world/browser",
    "deploy:firebase": "ng build --configuration production && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cli": "^18.2.4",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "bootstrap": "^3.3.7",
    "express": "^4.21.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "tslint": "^5.20.1",
    "typescript": "~5.5.2",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.4",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.1",
    "angular-eslint": "18.3.1",
    "eslint": "^9.9.1",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript-eslint": "8.2.0"
  },
  "engines": {
    "node": "20.12.2",
    "npm": "10.8.3"
  }
}
